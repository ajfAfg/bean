{erl_opts, [debug_info]}.

{alias, [{check, [xref, dialyzer]}]}.

{deps, []}.

{dialyzer, [{warnings, [overspecs]}, {get_warnings, true}, {plt_apps, top_level_deps}]}.

{xref_warnings, false}.

{xref_checks,
 [undefined_function_calls,
  undefined_functions,
  locals_not_used,
  exports_not_used,
  deprecated_function_calls,
  deprecated_functions]}.
